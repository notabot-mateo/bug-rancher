; Engine configuration file.
; Bug Rancher - Idle breeding game with mutant insects

config_version=5

[application]

config/name="Bug Rancher"
config/description="Breed, collect, and sell mutant bugs"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/game_manager.gd"
BugDatabase="*res://scripts/bug_database.gd"
Genetics="*res://scripts/genetics.gd"
HybridGen="*res://scripts/hybrid_generator.gd"
Upgrades="*res://scripts/upgrade_manager.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/handheld/orientation=1
window/stretch/mode="viewport"
window/stretch/aspect="keep_width"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
